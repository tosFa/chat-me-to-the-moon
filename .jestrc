{
  "browser": true,
  "testPathDirs": ["./tests/"],
  "verbose": true,
  "setupFiles": [
    "<rootDir>/tests/setup/index.js"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 85,
      "functions": 85,
      "lines": 85,
      "statement": 85
    }
  },
  "moduleNameMapper": {
    "\\.(css)$": "identity-obj-proxy"
  },
  "unmockedModulePathPatterns": [
    "node_modules/react/",
    "node_modules/enzyme/"
  ]

}